package com.academy;

import java.util.Scanner;

public class FibannociSeries {

	public static void main(String[] args) {


		Scanner sc = new Scanner(System.in);
		System.out.println("Enter no of digits");
		int c = sc.nextInt();
		int no1 = 0,no2=1;
		for(int i=1;i<=10;i++)
		{
		c=no1+no2;
		System.out.println(c);
		no1=no2;
		no2=c;
		
		
	}

}}
