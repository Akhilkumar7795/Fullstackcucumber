package com.academy;

public class GreaterThreeNumbers {

	public static void main(String[]args)
	{
		int no1=100;
		int no2 = 200;
		int no3= 300;
		if(no1>no2 && no2>no3)
		{
			System.out.println("no1 is greater");
		}
		if(no2>no3)
		{
			System.out.println("no2 is greater");
		}
		System.out.println("no3 is greater");
	}}
	
