package com.academy;

import java.util.Scanner;

public class Palindrome {

	public static void main(String[] args) {

		int no =121;int rem;int rev=0;
		int temp=no;
		
		while(no!=0) {
			
			rem=no%10;
			temp=rev*10+rem;
			no=no/10;
		}
		if(no==rev)
		{
			System.out.println("No is a Palindrome");
		}
		System.out.println("Not a Palindrome");
		}
	}
